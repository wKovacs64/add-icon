{
  "name": "@wkovacs64/add-icon",
  "version": "1.0.0",
  "description": "CLI tool to download and transform icons from Iconify",
  "keywords": [
    "iconify",
    "icons",
    "svg",
    "cli",
    "download"
  ],
  "author": "Justin R. Hall <justin.r.hall@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "add-icon": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "prepare": "npm run build",
    "typecheck": "attw --pack --profile esm-only",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "cm": "cz",
    "semantic-release": "semantic-release"
  },
  "prettier": "@wkovacs64/prettier-config",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "dependencies": {
    "@iconify/utils": "^2.3.0",
    "commander": "^13.1.0",
    "svgo": "^3.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@types/node": "22.14.1",
    "@wkovacs64/eslint-config": "7.5.2",
    "@wkovacs64/prettier-config": "4.1.1",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.24.0",
    "prettier": "3.5.3",
    "semantic-release": "24.2.3"
  }
}
